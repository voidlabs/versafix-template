  <template-def>

  <template tag="block">
    <def attr="bgcolor" selector="table" bgcolor="{{bgcolor}}" style="background-color: {{bgcolor}}"></def>
    <def attr="bgcolor-prop" selector="table" style="-ko-background-color: @[{{bgcolor-prop}}]; -ko-attr-bgcolor: @[{{bgcolor-prop}}]"></def>
    <def attr="name" selector="table" data-ko-block="{{name}}"></def>
    <def attr="label"></def>
    <def attr="theme"></def>
    <def attr="td-styles" selector="td" style="{{td-styles}}"></def>
    <def attr="height" selector="td" height="{{height}}" style="height: {{height}}px"></def>
    <def attr="height-prop" selector="td" style="-ko-height: @[{{height-prop}}]px; -ko-attr-height: @[{{height-prop}}]"></def>
    <tmpl>
    <!-- {{name}} -->
    <table role="presentation" class="vb-outer" width="100%" cellpadding="0" border="0" cellspacing="0">
      <tr><td class="vb-outer" align="center" valign="top" style="padding-left: 9px; padding-right: 9px; font-size: 0">{{replacedcontent}}</td></tr>
    </table>
    <!-- /{{name}} -->
    </tmpl>
  </template>

  <template tag="block-inner">
    <def attr="if" selector="div[data-ko-wrap]" data-ko-display="{{if}}"></def>
    <def attr="false" selector="div[data-ko-wrap]" style="display: none"></def>

    <def attr="inner-bgcolor" selector="div div table" bgcolor="{{inner-bgcolor}}" style="background-color: {{inner-bgcolor}}"></def>
    <def attr="inner-bgcolor-prop" selector="div div table" style="-ko-background-color: @[{{inner-bgcolor-prop}}]; -ko-attr-bgcolor: @[{{inner-bgcolor-prop}}]"></def>
    <def attr="inner-padding" selector="div div table" cellspacing="{{inner-padding}}" style="mso-cellspacing: {{inner-padding}}px; border-spacing: {{inner-padding}}px"></def>
    <def attr="inner-width" selector="div div table, cc-table" width="{{inner-width}}"></def>
    <!-- div[class] is required by old webkits not supporting max-width on tables -->
    <!-- Check on Safari for Windows v4.0.5 531.2.7 -->
    <def attr="inner-width" selector="div div table, div div" style="max-width: {{inner-width}}px; -mru-width: 0px"></def>
    <def attr="inner-class" selector="div div table" class="{{inner-class}}"></def>
    <!-- was display:inline-block", see https://github.com/voidlabs/versafix-template/issues/3 -->
    <tmpl>{{#if}}<div data-ko-wrap="false" style="width: 100%;">{{/if}}<!--[if (gte mso 9)|(lte ie 8)]><table role="presentation" align="center" border="0" cellspacing="0" cellpadding="0"><tr><td align="center" valign="top"><![endif]--><!--
      --><div style="margin: 0 auto"><table role="presentation" border="0" cellpadding="0" cellspacing="0" style="border-collapse: separate; width: 100%">
        {{replacedcontent}}
      </table></div><!--
    --><!--[if (gte mso 9)|(lte ie 8)]></td></tr></table><![endif]-->{{#if}}</div>{{/if}}</tmpl>
  </template>

  <template tag="columns">
    <def attr="autowidth"></def>
    <def attr="width" selector="cc-table" width="{{width}}"></def>
    <!-- use a div inside the TD so that columns widths are handled correctly when the horizontal spacing of the container is different from its vertical spacing and the content should not fill 100% of the space but leave some spacing -->
    <def attr="width" selector="div" style="max-width: {{width}}px; -mru-width: 0px"></def>
    <def attr="td-styles" selector="td" style="{{td-styles}}"></def>
    <tmpl><tr>
      <td align="center" valign="top" style="font-size: 0"><div style="width:100%"><!--[if (gte mso 9)|(lte ie 8)]><table role="presentation" align="center" border="0" cellspacing="0" cellpadding="0"><tr><![endif]--><!--
        -->{{replacedcontent}}<!--
      --><!--[if (gte mso 9)|(lte ie 8)]></tr></table><![endif]--></div></td>
    </tr></tmpl>
  </template>


  <template tag="content">
    <def attr="width" selector="table" width="{{width}}"></def>
    <def attr="width" selector="div" style="max-width: {{width}}px; -mru-width: 0px"></def>
    <def attr="width-prop" selector="table" style="-ko-attr-width: @[{{width-prop}}]"></def>
    <!-- uso la forma con espressione perchè mosaico ha un bug nella ricerca del valore di default e non lo cerca nella prop precedente -->
    <def attr="width-prop" selector="div" style="-ko-max-width: @[{{width-prop}} + 'px']"></def>

    <!-- border-spacing forzato per orange -->
    <def attr="spacing" selector="table" cellspacing="{{spacing}}" style="mso-cellspacing: {{spacing}}px; border-spacing: {{spacing}}px"></def>
    <def attr="align" selector="table" align="{{align}}"></def>
    <def attr="text-align" selector="table" style="text-align: {{text-align}}"></def>

    <def attr="class" selector="div" class="{{class}}"></def>
    <def attr="td-styles" selector="td" style="{{td-styles}}"></def>

    <!-- the div used to have a  display: inline-block style. Removed it trying to remove unwanted space on iOS 10 -->
    <tmpl><tr>
      <td align="center" valign="top" style="font-size: 0"><div style="vertical-align:top; width:100%"><!--
        --><table role="presentation" class="vb-content" border="0" cellspacing="0" cellpadding="0" style="border-collapse: separate; width: 100%">
          {{replacedcontent}}
        </table></div></td>
    </tr></tmpl>
  </template>


  <template tag="column-content">
    <def attr="if" selector="div[data-ko-wrap], cc-td" data-ko-display="{{if}}"></def>
    <def attr="false" selector="div[data-ko-wrap], cc-td" style="display: none"></def>

