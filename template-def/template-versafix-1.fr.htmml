    <def attr="href" selector="a" href="{{href}}"></def>

    <def attr="bgcolor" selector="table td" bgcolor="{{bgcolor}}" style="background-color: {{bgcolor}}"></def>
    <def attr="color" selector="table td, a" style="color: {{color}}"></def>
    <def attr="size" selector="table td, a" style="font-size: {{size}}"></def>
    <def attr="face" selector="table td, a" style="font-family: {{face}}"></def>
    <def attr="radius" selector="table td" style="border-radius: {{radius}}px"></def>
    <def attr="style" selector="table td" style="-ko-border-radius: @[{{style}}.radius]px; -ko-attr-bgcolor: @{{style}}.buttonColor; -ko-background-color: @{{style}}.buttonColor"></def>
    <!-- Libero.it webmail alters font-size for a tags not defining font sizes inline -->
    <def attr="style" selector="table td, a" style="-ko-font-size: @[{{style}}.size]px; -ko-color: @{{style}}.color; -ko-font-family: @{{style}}.face"></def>
    <def attr="style" selector="td[valign], table" style="{{#align}}-ko-attr-align: @{{style}}.align{{/align}}"></def>

