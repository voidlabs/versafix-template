            <linked-img false="" if="imageVisible and fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="166" width-prop="184 - blockSpacing.horizontalSpacing * 2" placeholder-height="{{height}}" padding-bottom="8" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C{{height}}"></linked-img>
