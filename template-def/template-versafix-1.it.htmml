          <divider width="100" height="1" style="hrStyle"></divider>
        </content>
      </block-inner>
    </block>

    <block name="buttonBlock" label="Button Block">
      <block-inner>
        <content>
          <btn editable="link" inner-padding="12" align="center" inner-hpadding="14" size="22px" style="bigButtonStyle">PULSANTE</btn>
