            </column-content><!--
          --></repeater></columns>
      </block-inner>
      <block-inner false="" if="gutterVisible">
        <columns><repeater repeat="left,right"><!--
            --><column-content width="276">
              <linked-img if="fixedImageHeightVisible" editable="{{repeat}}Image" width="258" width-prop="276 - blockSpacing.horizontalSpacing * 2" height="180" height-prop="imageHeight" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C180"></linked-img>
              <linked-img false="" if="fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="258" width-prop="276 - blockSpacing.horizontalSpacing * 2" placeholder-height="180" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C180"></linked-img>
