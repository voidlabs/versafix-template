    <!-- Note: use content-spacing-style-with-padding-bottom instead of content-spacing-style when using this -->
    <def attr="padding-bottom" selector="td" style="padding-bottom: {{padding-bottom}}px"></def>

    <def attr="content-spacing" selector="td" style="{{content-spacing}}"></def>
    <def attr="content-spacing-style" selector="td" style="padding: 9px; padding-top: 5px; padding-bottom: 5px; -ko-padding: @[{{content-spacing-style}}.horizontalSpacing]px; -ko-padding-top: @[{{content-spacing-style}}.verticalSpacing]px; -ko-padding-bottom: @[{{content-spacing-style}}.verticalSpacing]px;"></def>

    <!-- ugly hack, but I have not better way to deal with conflicting attribute combinations: 13px is hardcoded here 
         because htmml can't calculate 5px + {{padding-bottom}} and we know padding-bottom is always 8px here -->
    <def attr="content-spacing-style-with-padding-bottom" selector="td" style="-ko-padding: @[{{content-spacing-style-with-padding-bottom}}.horizontalSpacing]px; -ko-padding-top: @[{{content-spacing-style-with-padding-bottom}}.verticalSpacing]px; -ko-padding-bottom: @[Math.round({{padding-bottom}}) + Math.round({{content-spacing-style-with-padding-bottom}}.verticalSpacing)]px;"></def>

