    <block-inner>
      <content>
        <long-text editable="longText" fixed-align="center"><p>Email inviata a <a href="mailto:[mail]">[mail]</a></p></long-text>
        <linked-text align="center" href="[unsubscribe_link]" editable="disiscrivitiText" color-prop="Color.readability(linkStyle.color, backgroundColor) gt 2 ? linkStyle.color : (Color.isReadable('#ffffff', backgroundColor) ? '#ffffff' : '#000000')">Disiscriviti</linked-text>
        <linked-img width="170" align="center" false="" if="_root_.sponsor.visible" content="_root_.sponsor" src="img/sponsor.gif" alt="sponsor"></linked-img>
