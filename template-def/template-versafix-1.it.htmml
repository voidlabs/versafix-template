      </block-inner>
      <block-inner false="" if="gutterVisible">
        <content>
          <linked-img editable="image" width="534" width-prop="552 - 2 * blockSpacing.horizontalSpacing" placeholder-height="280" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=534%2C280"></linked-img>
        </content>
      </block-inner>
