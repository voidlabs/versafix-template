    <def attr="content-spacing-style" selector="td[valign='top']" style="padding: 9px; padding-top: 5px; padding-bottom: 5px; -ko-padding: @[{{content-spacing-style}}.horizontalSpacing]px; -ko-padding-top: @[{{content-spacing-style}}.verticalSpacing]px; -ko-padding-bottom: @[{{content-spacing-style}}.verticalSpacing]px;"></def>

