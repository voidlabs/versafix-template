    <def attr="alt" selector="img" alt="{{alt}}"></def>
    <def attr="href-prop" selector="a" style="-ko-attr-href: @[{{href-prop}}]"></def>
    <tmpl><div data-ko-wrap="false" style="display: inline-block;">&nbsp;<a href=""><img border="0" style="display: inline-block; vertical-align: top; padding-bottom: 0px"></a></div></tmpl>
  </template>

