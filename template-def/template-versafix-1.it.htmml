    <block name="bigSocialBlock" label="Big Social Block">
      <block-inner>
        <content>
          <custom-row align="center" class="links-color socialLinks mobile-textcenter">
            <repeater prefix="fb,tw,gg,web,wa,tg,in,pi,fl,vi,inst,you,tt,tc" default="if,if,false if,false if,false if,false if,if,false if,false if,false if,if,if,false if,false if" color="#3b5998,#4099FF,#d34836,#606060,#25d366,#2da5e1,#007bb6,#C92228,#ff0084,#45bbff,#bc2a8d,#cd201f,#000000,#9146ff" name="Facebook,Twitter,Google,Web,Whatsapp,Telegram,Linkedin,Pinterest,Flickr,Vimeo,Instagram,Youtube,TikTok,Twitch">
              <social-icon {{default}}="{{prefix}}Visible" href-prop="{{prefix}}Url" size="48" size-prop="bigSocialIconSize" src="img/icons/{{prefix}}-rdcol-96.png" src-prop="Url.templatePath('img/icons/{{prefix}}-'+bigSocialIconType+'-96.png')" alt="{{name}}"></social-icon>
            </repeater>
          </custom-row>
        </content>
      </block-inner>
    </block>

    <block name="shareBlock" label="Share Block">
      <block-inner if="shareButtonType eq 'reverse'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,false if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
            <column-content {{default}}="{{prefix}}Visible" align="center">
              <share-btn href="{{shareurl}}%5Bpermlink_urlenc%5D" color="#FFFFFF" bgcolor="{{color}}" src="img/icons/{{prefix}}-white-96.png" btn-style="shareButtonStyle" editable="{{prefix}}Text">Condividi</share-btn>
            </column-content>
          </repeater>
        </columns>
      </block-inner>
      <block-inner false="" if="shareButtonType eq 'simple'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,false if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
            <column-content {{default}}="{{prefix}}Visible" align="center">
              <share-btn href="{{shareurl}}%5Bpermlink_urlenc%5D" color="#000000" bgcolor="#FFFFFF" src="img/icons/{{prefix}}-coloured-96.png" font-style="shareButtonStyle" editable="{{prefix}}Text">Condividi</share-btn>
            </column-content>
          </repeater>
        </columns>
      </block-inner>
      <block-inner false="" if="shareButtonType eq 'custom'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,false if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
