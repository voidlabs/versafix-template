  <title style="-ko-bind-text: @titleText">TITOLO</title>
  <style type="text/css">
    @supports -ko-blockdefs {
      id { widget: id }
      size { label: Dimensioni; widget: integer; min: 8; max: 60; }
      visible { label: Visibile?; widget: boolean }
      color { label: Colore; widget: color }
      radius {
        label: Arrotondamento;
        widget: integer;
        max: 40;
        help: Attenzione - questa proprietà non è supportata da tutti i client di posta (ad esempio Outlook)
      }
      face { label: Font; widget: select; options: Arial, Helvetica, sans-serif=Arial|Arial Black, Arial Black, Gadget, sans-serif=Arial Black|Comic Sans MS, Comic Sans MS5, cursive=Comic Sans|Courier New, Courier New, monospace=Courier|Georgia, serif=Georgia|Impact, sans-serif=Impact|Lucida Console, Monaco, monospace=Lucida Console|Lucida Sans Unicode, Lucida Grande, sans-serif=Lucida Sans Unicode|Times New Roman, Times, serif=Times New Roman|Verdana, Geneva, sans-serif=Verdana;
        renderHint: fontface
      }
      decoration { label: Sottolineatura; widget: select; options: none=Nessuna|underline=Sottolineato }
      linksColor { label: Colore link; extend: color }
      linksDecoration { label: Link sottolineati?; extend: decoration }
      buttonColor { label: Colore Pulsante; extend: color; transparent: true }
      text { label: Paragrafo; widget: text }
      url { label: Link; widget: url }
      src { label: Immagine; widget: src }
      hrWidth { label: Larghezza; widget: integer; min: 5; max: 100; step: 5; }
      hrHeight { label: Spessore linea; widget: integer; max: 80; }

      height { label: Altezza; widget: integer }
      imageHeight { label: Altezza immagine; extend: height; min: 10; max: 1000 }
      lineHeight { label: Altezza; widget: select; options: normal=Normale|150%=1.5 Linee|200%=Doppia;
        buttonsetLabels: normal=1x|150%=1.5x|200%=2x;
      }

      spacerSize { label: Altezza; widget: integer; max: 90; min: 4; }
      align { label: Allineamento; widget: select; options:left=Sinistra|center=Centrato|right=Destra;
        buttonsetIconClasses: left=fa fa-fw fa-align-left|center=fa fa-fw fa-align-center|right=fa fa-fw fa-align-right
      }
      alt {
        label: Testo alternativo;
        widget: text;
        help: Il testo alternativo viene visualizzato dai programmi che non caricano le immagini automaticamente;
      }
      sponsor { label: Sponsor; properties: visible=true src url alt; category: hidden }
      titleText { label: Testo titolo; category: hidden }
      gutterVisible { label: Mostra spaziatura; extend: visible }
      socialIconType { label: Versione icone; widget: select; options: bw=Bianco e Nero|colors=Colori;
        renderHint: images; 
        imagesSources: bw=url('edres/_social-select-bw.png')
                      |colors=url('edres/_social-select-colors.png')
      }
      bigSocialIconType { label: Versione icone; widget: select; 
        options: bw=Cerchio Grigio
                |colors=Cerchio Colorato
                |bwb=Cerchio Nero
                |sqbw=Quadrato Grigio
                |sqcolors=Quadrato Colorato
                |sqbwb=Quadrato Nero
                |rdcol=Cerchiate a colori
                |rdbl=Cerchiate nere
                |rdw=Cerchiate bianche
                |sqrdcol=Squadrate a colori
                |sqrdbl=Squadrate nere
                |sqrdw=Squadrate bianche
                |white=Bianche
                |coloured=Colorate
                |black=Nere;
        renderHint: images; 
        imagesSources: bw=url('edres/_social-select-bw.png')
                      |colors=url('edres/_social-select-colors.png')
                      |bwb=url('edres/_social-select-bwb.png')
                      |sqbw=url('edres/_social-select-sqbw.png')
                      |sqcolors=url('edres/_social-select-sqcolors.png')
                      |sqbwb=url('edres/_social-select-sqbwb.png')
                      |rdcol=url('edres/_social-select-rdcol.png')
                      |rdbl=url('edres/_social-select-rdbl.png')
                      |rdw=url('edres/_social-select-rdw.png')
                      |sqrdcol=url('edres/_social-select-sqrdcol.png')
                      |sqrdbl=url('edres/_social-select-sqrdbl.png')
                      |sqrdw=url('edres/_social-select-sqrdw.png')
                      |white=url('edres/_social-select-white.png')
                      |coloured=url('edres/_social-select-coloured.png')
                      |black=url('edres/_social-select-black.png');
      }
      bigSocialIconSize { label: Dimensioni icone; widget: select; options: 32=Piccole|48=Grandi; renderHint: buttonset }

      preheaderLinkOption {
        label: Link disiscrizione;
        widget: select;
        options: [profile_link]=Profilo |[unsubscribe_link]=Disiscrizione|none=Nessuno;
        help: Se viene selezionato -Nessuno- verrà visualizzato il testo di preintestazione;
      }
     
      hrStyle { label: Stile separatore; properties: color hrWidth hrHeight; }
      hrStyle:preview { height: 200%; width: 200%; bottom: 20px; -ko-border-bottom: @[hrHeight]px solid @color; }
      preheaderVisible { label: Mostra Preintestazione; extend: visible; help: Il blocco preintestazione è il primo blocco in alto nella pagina. Contiene il link per la visualizzazione del messaggio sul web ed eventualmente il link per la disiscrizione o un testo che verrà visualizzato in anteprima da alcuni programmi di posta }

      /* content types */
      blocks { label: Blocchi; properties: blocks[]; }
      link { label: Link; properties: text url }
      image { label: Immagine; properties: src url alt }
      backgroundColor { label: Colore sfondo; extend: color }
      buttonLink { label: Pulsante; extend: link }
      
      /* texts and links */
      textStyle { label: Testo; properties: face color size align }
      textStyle:preview { -ko-bind-text: @['AaZz']; -ko-font-family: @face; -ko-color: @color; -ko-font-size: @[size]px; }
      linkStyle { label: Link; properties: face color size decoration=none }
      linkStyle:preview { -ko-bind-text: @['Link']; -ko-font-size: @[size]px; -ko-font-family: @face; -ko-color: @color; -ko-text-decoration: @[decoration] }
      longTextStyle { label: Paragrafo; properties: face color size lineHeight align linksColor   }
      longTextStyle:preview { -ko-bind-text: @['AaZz']; -ko-font-family: @face; -ko-color: @color; -ko-font-size: @[size]px; }
      bigButtonStyle { label: Pulsante grande; extend: buttonStyle }
      titleTextStyle { label: Titolo; extend: textStyle }
      /* background */
      externalBackgroundColor { label: Sfondo esterno; extend: color }

      externalTextStyle { label: Testo alternativo; extend: textStyle }
      externalTextStyle:preview { -ko-bind-text: @['AaZz']; -ko-font-family: @face; -ko-color: @color; -ko-font-size: @[size]px; }
     
      bigTitleStyle { label: Titolo; properties: face color size align}
      bigTitleStyle:preview { -ko-bind-text: @['AaZz']; -ko-font-family: @face; -ko-color: @color; -ko-font-size: @[size]px; }
      /* buttons */
      buttonStyle color { label: Colore testo; extend: color; transparent: true; }
      buttonStyle size { label: Dimensione testo; extend: size }
      buttonStyle borderWidth { label: Spessore Bordo; widget: integer; min: 0; max: 10; }
      buttonStyle borderColor { label: Colore Bordo; }
      buttonStyle verticalPadding { label: Spazio verticale; widget: integer; min: 0; max: 40; }
      buttonStyle horizontalPadding { label: Spazio orizzontale; widget: integer; min: 0; max: 40; }
      buttonStyle { label: Pulsante; properties: face color size align buttonColor radius borderWidth borderColor verticalPadding horizontalPadding }
      buttonStyle:preview { -ko-bind-text: @['Pulsante']; -ko-font-family: @face; -ko-color: @color; -ko-font-size: @[size]px; -ko-background-color: @buttonColor; padding-left: 5px; -ko-border-radius: @[radius]px; }

      blockSpacing verticalSpacing { label: Spazio verticale; widget: integer; min: 0; max: 40 }
      blockSpacing horizontalSpacing { label: Spazio orizzontale; widget: integer; min: 0; max: 40 }
      blockSpacing topPadding { label: Margine superiore; widget: integer; min: 0; max: 40 }
      blockSpacing bottomPadding { label: Margine inferiore; widget: integer; min: 0; max: 40 }
      blockSpacing { label: Spaziature; properties: topPadding bottomPadding verticalSpacing horizontalSpacing; category: style /* make sure this ends up as a style property, with inheritance/theming */ }
      
      /* contents */
      preheaderText {label: Testo Preintestazione; extend:text; help: Testo mostrato come preview del contenuto in alcuni client di posta;}
      leftImage { label: Imm. sinistra; extend: image }
      leftLongText { label: Testo sinistra; extend: text }
      leftButtonLink { label: Pulsante sinistra; extend: buttonLink }
      middleImage { label: Imm. centrale; extend: image }
      middleLongText { label: Testo centrale; extend: text }
      middleButtonLink { label: Pulsante centrale; extend: buttonLink }
      rightImage { label: Imm. destra; extend: image }
      rightLongText { label: Testo destra; extend: text }
      rightButtonLink { label: Pulsante destra; extend: buttonLink }
      webversionText{ label: Testo link web; extend: text;}
      unsubscribeText{ label: Testo unsubscribe; extend: text;}

      titleVisible { label: Mostra Titolo; extend: visible; }
      buttonVisible { label: Mostra Pulsante; extend: visible; }
      imageVisible { label: Mostra Immagine; extend: visible; }

      contentTheme { label: Stile Corpo; }
      contentTheme:preview { -ko-background-color: @[backgroundColor] }
      frameTheme { label: Stile Cornice; }
      frameTheme:preview { -ko-background-color: @[backgroundColor] }
      template preheaderText { label: Preheader; }

      template { label: Pagina; theme: frameTheme; properties: preheaderVisible=true; version: __VERSION__; }

      footerBlock { label: Blocco Disiscrizione; theme: frameTheme }
      
      fbVisible { label: Facebook; extend: visible }
      twVisible { label: Twitter; extend: visible }
      ggVisible { label: Google+; extend: visible }
      inVisible { label: LinkedIn; extend: visible }
      piVisible { label: Pinterest; extend: visible }
      flVisible { label: Flickr; extend: visible }
      viVisible { label: Vimeo; extend: visible }
      webVisible { label: Website; extend: visible }
      waVisible { label: Whatsapp; extend: visible }
      tgVisible { label: Telegram; extend: visible }
      instVisible { label: Instagram; extend: visible }
      youVisible { label: YouTube; extend: visible }
      ttVisible { label: TikTok; extend: visible }
      tcVisible { label: Twitch; extend: visible }
      fbUrl { label: Indirizzo Facebook; extend: url }
      twUrl { label: Indirizzo Twitter; extend: url}
      ggUrl { label: Indirizzo Google+; extend: url}
      inUrl { label: Indirizzo LinkedIn; extend: url}
      piUrl { label: Indirizzo Pinterest; extend: url}
      flUrl { label: Indirizzo Flickr; extend: url}
      viUrl { label: Indirizzo Vimeo; extend: url}
      webUrl { label: Indirizzo Website; extend: url}
      waUrl { label: Indirizzo Whatsapp; extend: url}
      tgUrl { label: Indirizzo Telegram; extend: url}
      instUrl { label: Indirizzo Instagram; extend: url}
      youUrl { label: Indirizzo YouTube; extend: url}
      ttUrl { label: Indirizzo TikTok; extend: url}
      tcUrl { label: Indirizzo Twitch; extend: url}

      socialBlock {
        label: Blocco social footer;
        properties: socialIconType=colors fbVisible=true fbUrl twVisible=true twUrl ggVisible=false ggUrl webVisible=false webUrl waVisible=false waUrl tgVisible=false tgUrl inVisible=false inUrl piVisible=false piUrl flVisible=false flUrl viVisible=false viUrl instVisible=true instUrl youVisible=false youUrl ttVisible=false ttUrl tcVisible=false tcUrl longTextStyle longText backgroundColor;
        variant:socialIconType;
        theme: frameTheme
      }

      bigSocialBlock {
        label: Blocco social;
        properties: bigSocialIconType=rdcol bigSocialIconSize=48 fbVisible=true fbUrl twVisible=true twUrl ggVisible=false ggUrl webVisible=false webUrl waVisible=false waUrl tgVisible=false tgUrl inVisible=true inUrl piVisible=false piUrl flVisible=false flUrl viVisible=false viUrl instVisible=true instUrl youVisible=true youUrl ttVisible=false ttUrl longTextStyle tcVisible=false tcUrl longTextStyle longText;
        variant: bigSocialIconType;
        theme: contentTheme
      }

      shareButtonStyle iconColorType { label: Colore icone; widget: select; options: brand=Colori|white=Bianco|black=Nero; }
      shareButtonStyle color { label: Colore testo; extend: color; transparent: true; }
      shareButtonStyle size { label: Dimensione testo; widget: integer; min: 10; max: 25; }
      shareButtonStyle { label: Pulsante condivisione; properties: face iconColorType=black color size align buttonColor radius }
      shareButtonStyle:preview { -ko-bind-text: @['Pulsante']; -ko-font-family: @face; -ko-color: @['black']; -ko-font-size: @[size]px; -ko-background-color: @['#CCCCCC']; padding-left: 5px; -ko-border-radius: @[radius]px; }

      shareButtonType { label: Tipo pulsanti;widget: select; options:reverse=Colorati|simple=Icone colorate|custom=Personalizzati; }
      shareBlock fbVisible { label: Facebook; }
      shareBlock twVisible { label: Twitter }
      shareBlock inVisible { label: LinkedIn }
      shareBlock ggVisible { label: Google+ }
      shareBlock piVisible { label: Pinterest }
      shareBlock customUrl { label: Indirizzo condivisione; }
      shareBlock {
        label: Blocco condivisione;
        properties: shareButtonType=reverse fbVisible=true twVisible=true inVisible=false ggVisible=false piVisible=false customUrl shareButtonStyle;
        variant: shareButtonType;
        theme: contentTheme;
      }
      
      preheaderBlock { label:Blocco Preintestazione; theme: frameTheme; visibility: _root_.preheaderVisible; /* this is a fixed block:  using "visibility" property link this to the boolean variable that deal with its visibility and let mosaico add a trash button to hide it "inline" */ }

      sideArticleBlock imagePos {label:Posizione immagine;widget:select; options: left=Sinistra|right=Destra;
        buttonsetIconClasses: left=fa fa-fw fa-address-card-o|right=fa fa-fw fa-address-card-o fa-flip-horizontal
      }
      sideArticleBlock imageWidth { label: Dimensioni Immagine; widget: select; options: 120=Piccola|166=Media|258=Grande;
        renderHint: buttonset;
      }
      sideArticleBlock { label: Blocco Immagine+Testo; properties: backgroundColor titleVisible=true buttonVisible=true imageWidth=166 imagePos=left titleTextStyle longTextStyle buttonStyle  image  longText buttonLink; variant:imagePos; theme: contentTheme }

      sideImageBlock imagePos {label:Posizione immagine;widget:select; options: left=Sinistra|right=Destra;
        buttonsetIconClasses: left=fa fa-fw fa-address-card-o|right=fa fa-fw fa-address-card-o fa-flip-horizontal
      }
      sideImageBlock imageWidth { label: Dimensioni Immagine; widget: select; options: 143=Mini|190=Piccola|285=Media|380=Grande|427=Molto grande; }
      sideImageBlock { label: Blocco Immagine+Testo; properties: backgroundColor titleVisible=true buttonVisible=true imageWidth=285 imagePos=left titleTextStyle longTextStyle buttonStyle  image  longText buttonLink; variant:imagePos; theme: contentTheme }

      textBlock { label: Blocco Testo; properties: backgroundColor longTextStyle longText; theme: contentTheme}

      singleArticleBlock imageWidth { label: Dimensioni Immagine; widget: select; options: 166=Piccola|258=Media|350=Grande|534=Intera }
      singleArticleBlock { label: Blocco Immagine/Testo; properties: titleVisible=true buttonVisible=true imageVisible=true imageWidth=534 titleTextStyle longTextStyle buttonStyle  image  longText buttonLink; theme: contentTheme}

      fixedImageHeightVisible { label: Blocca altezza immagini; extend: visible; }
      externalBackgroundVisible { label: Sfondo trasparente; extend: visible; }

      doubleArticleBlock { label: Blocco 2 Colonne; properties: backgroundColor titleVisible=true buttonVisible=true imageVisible=true fixedImageHeightVisible=true titleTextStyle longTextStyle buttonStyle  leftImage  leftLongText leftButtonLink rightImage  rightLongText rightButtonLink; theme: contentTheme}

      tripleArticleBlock { label: Blocco 3 Colonne; properties: backgroundColor titleVisible=true buttonVisible=true imageVisible=true fixedImageHeightVisible=true titleTextStyle longTextStyle buttonStyle  leftImage  leftLongText leftButtonLink middleImage  middleLongText middleButtonLink rightImage  rightLongText rightButtonLink; theme: contentTheme}
      
      logoBlock longTextStyle { label: Testo alternativo; }
      logoBlock imageWidth { label: Dimensioni Immagine; widget: select; options: 166=Piccola|258=Media|350=Grande;
        renderHint: buttonset;
      }
      logoBlock { label: Blocco Logo; properties: externalBackgroundVisible=true image imageWidth=258 externalTextStyle; variant: externalBackgroundVisible; theme: contentTheme}
      
      /* a deprecated block is a block that is "known", so loaded models will still work, but is not available in the block list */
      titleBlock { label: Titolo; properties: bigTitleStyle; theme: contentTheme; deprecated: true }
      titleBlock2 { label: Titolo; properties: bigTitleStyle; theme: contentTheme}

      imageBlock longTextStyle { label: Testo alternativo; }
      imageBlock { label: Immagine; properties: gutterVisible=false; variant: gutterVisible; theme: contentTheme }

      doubleImageBlock longTextStyle { label: Testo alternativo; }
      doubleImageBlock { label: Blocco Galleria due Immagini; properties: gutterVisible=false fixedImageHeightVisible=true; variant: gutterVisible; theme: contentTheme }

      tripleImageBlock longTextStyle { label: Testo alternativo; }
      tripleImageBlock { label: Blocco Galleria tre Immagini;properties:gutterVisible=false fixedImageHeightVisible=true; variant: gutterVisible; theme: contentTheme}

      buttonBlock { label: Blocco Pulsante; properties: bigButtonStyle; theme: contentTheme}
      hrBlock { label: Blocco Separatore; properties: hrStyle; theme: contentTheme; deprecated: true }
      hrBlock2 { label: Blocco Separatore; properties: hrStyle; theme: contentTheme }
      spacerBlock { label: Blocco Spaziatore; properties: externalBackgroundVisible=true; variant: externalBackgroundVisible; theme: contentTheme }

      spacerBlock:preview,
      logoBlock:preview { -ko-background-color: @[externalBackgroundColor] }

      preheaderBlock:preview,
      hrBlock:preview,
      hrBlock2:preview,
      sideArticleBlock:preview,
      sideImageBlock:preview,
      textBlock:preview,
      singleArticleBlock:preview,
      doubleArticleBlock:preview,
      tripleArticleBlock:preview,
      titleBlock:preview,
      titleBlock2:preview,
      footerBlock:preview,
      socialBlock:preview,
      buttonBlock:preview,
      titleBlock:preview { -ko-background-color: @[backgroundColor] }
    }
  </style>
  <style type="text/css" data-ko-remove="">
    /* PREVIEW STYLES */
    .fullpreview [data-ko-block='hrBlock'],
    .fullpreview [data-ko-block='titleBlock'] {
      display: none;
    }
  </style>
  <style type="text/css" data-inline="true">
    body { margin: 0; padding: 0; }
    img { border: 0px; display: block; }

    .socialLinks { font-size: 6px; }
    .socialLinks a {
      display: inline-block;
    }

    .long-text p { margin: 1em 0px; }
    .long-text p:last-child { margin-bottom: 0px; }
    .long-text p:first-child { margin-top: 0px; }
  </style>
  <style type="text/css">
    /* yahoo, hotmail */
    .ExternalClass, .ExternalClass p, .ExternalClass span, .ExternalClass font, .ExternalClass td, .ExternalClass div { line-height: 100%; }
    .yshortcuts a { border-bottom: none !important; }
    .vb-outer { min-width: 0 !important; }
    .RMsgBdy, .ExternalClass {
      width: 100%;
      background-color: #3f3f3f;
      -ko-background-color: @[_theme_.frameTheme.backgroundColor]
    }

    /* outlook/office365 add buttons outside not-linked images and safari have 2px margin */
    [o365] button { margin: 0 !important; }

    /* outlook */
    table { mso-table-rspace: 0pt; mso-table-lspace: 0pt; }
    #outlook a { padding: 0; }
    img { outline: none; text-decoration: none; border: none; -ms-interpolation-mode: bicubic; }
    a img { border: none; }

    @media screen and (max-width: 600px) {
      table.vb-row {
        width: 95% !important;
      }

      .mobile-hide { display: none !important; }
      .mobile-textcenter { text-align: center !important; }

      .mobile-full { 
        width: 100% !important;
        max-width: none !important;
      }
    }
    /* previously used also screen and (max-device-width: 600px) but Yahoo Mail doesn't support multiple queries */
  </style>
