  <default selector="[theme='frameTheme'] block-inner.multicolumns columns" mso-vertical-inner-padding="4"></default>
  <!-- preHeaderBlock -->
  <default selector="[theme='frameTheme'] block-inner:not(.multicolumns):not(.gutterless) columns" vertical-inner-padding="4"></default>
  <!-- tripleArticleBlock, doubleArticleBlock, partially socialBlock -->
  <default selector="block-inner.multicolumns columns" mso-vertical-inner-padding="13"></default>
  <default selector="block-inner:not(.gutterless) columns" horizontal-inner-padding="9"></default>
  <default selector="block-inner:not(.multicolumns):not(.gutterless) columns" vertical-inner-padding="13"></default>

  <default selector="[theme='frameTheme'] .multicolumns column-content" notmso-vertical-inner-padding="4"></default>
  <default selector=".multicolumns column-content" notmso-vertical-inner-padding="13"></default>
  <!-- force border-spacing to 0 for Sfr.fr/Orange.fr -->
  <default selector=".gutterless column-content" spacing="0"></default>
  <default selector="column-content" spacing="9" align="left"></default>

  <default selector="block[theme='frameTheme'] linked-text" color="#ffffff" decoration="underline" style="linkStyle"></default>
  <default selector="block[theme='frameTheme'] long-text"></default>
  <default selector="block[theme='frameTheme'] text, block[theme='frameTheme'] long-text" color="#919191" style="longTextStyle"></default>


  <default selector="block[theme='contentTheme'] text" size="18px" style="titleTextStyle"></default>
  <default selector="block[theme='contentTheme'] long-text" line-height="normal" style="longTextStyle"></default>
  <default selector="share-btn" inner-addhpadding="4px" icon-size="24" icon-alt="{{name}}"></default>
  <default selector="block[theme='contentTheme'] btn" inner-hpadding="18" style="buttonStyle"></default>
  <default selector="block[theme='contentTheme'] btn, share-btn" inner-padding="6" bgcolor="#bfbfbf" border-radius="4"></default>
  <default selector="block[theme='contentTheme'] linked-img" style="longTextStyle"></default>

  <default selector="divider" color="#3f3f3f"></default>
  <default selector="text, title-text, long-text, share-btn, btn, linked-text, linked-img" face="Arial, Helvetica, sans-serif" color="#3f3f3f" size="13px"></default>
  <default selector="text, title-text, long-text:not([fixed-align]), btn, linked-text" align="left"></default>

  <!-- some linked-img is used gutterless containers, so we don't declare them here -->
  <default selector="text, title-text, long-text, share-btn, btn, linked-text, custom-row, divider" content-spacing="padding: 9px; padding-top: 5px; padding-bottom: 5px"></default>
  <default selector="linked-img[padding-bottom]" content-spacing="padding: 9px; padding-top: 5px; padding-bottom: 13px"></default>
  <default selector="block-inner:not(.gutterless) linked-img:not([padding-bottom])" content-spacing="padding: 9px; padding-top: 5px; padding-bottom: 5px"></default>

  <!-- immages on gallery blocks are shown full width (100%) on mobile -->
  <default selector="*[if='gutterVisible eq false'] linked-img" class="mobile-full"></default>

  <default selector="social-icon" size="32" radius="50"></default>


<!--
    <block name="imageSeparatorBlock" label="Image Separator">
      <block-inner class="gutterless" inner-bgcolor="#bfbfbf" inner-bgcolor-prop="externalBackgroundColor">
        <linked-img width="570" height="52"
          src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=570%2C52"
          src-prop="'img?method=magic&params=0,0&src=uploads/fgbg.png&src2=uploads/shadow2.png&white='+externalBackgroundColor+'&black='+backgroundColor"></linked-img>
        <linked-img width="570" height="52"
          src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=570%2C52"
          src-prop="'img?method=colorize&params='+backgroundColor+',%23FF00FF,90&src=uploads/fgbg-colorize.png'"></linked-img>
      </block-inner>
    </block>
-->

  <!-- Note: 
    the conditional comment around the body tag (and /body) is to hide the whole tag to windows 10 mail to prevent really weird bugs with windows 10 mail.
    the conditional comment also remove the tag from Outlook 2016 and following versions, as they share the same version of windows 10 mail.
    2022-06-09: switched to gte mso 15 to remove the body also to Outlook 2013 so 1px lines are white instead of the external frame color.
  -->

  </template-def>
</head>
<!--[if !(gte mso 15)]-->
<body bgcolor="#3f3f3f" text="#919191" alink="#cccccc" vlink="#cccccc" style="background-color: #3f3f3f; color: #919191;
  -ko-background-color: @_theme_.frameTheme.backgroundColor; -ko-attr-bgcolor: @_theme_.frameTheme.backgroundColor; -ko-color: @_theme_.frameTheme.longTextStyle.color;
  -ko-attr-text: @_theme_.frameTheme.longTextStyle.color; -ko-attr-alink: @_theme_.frameTheme.longTextStyle.linksColor;
  -ko-attr-vlink: @_theme_.frameTheme.longTextStyle.linksColor"><!--<![endif]--><center>

  <div data-ko-display="preheaderVisible" data-ko-wrap="false">

    <block name="preheaderBlock" label="Preheader Block" theme="frameTheme">
      <div data-ko-display="preheaderLinkOption neq 'none'" style="display: none; font-size:1px; line-height: 1px; max-height:0px; max-width: 0px; opacity: 0; overflow: hidden; 
          -ko-bind-text: @preheaderText"></div>
      <block-inner>
        <columns><!--
          --><column-content width="276">
            <linked-text if="preheaderLinkOption neq 'none'" href="[unsubscribe_link]" href-prop="preheaderLinkOption" editable="unsubscribeText">Unsubscribe</linked-text>
            <text false="" if="preheaderLinkOption eq 'none'" content="preheaderText"></text>
          </column-content><!--
          --><column-content width="276" class="mobile-hide">
            <linked-text align="right" href="[show_link]" editable="webversionText">View in your browser</linked-text>
          </column-content><!--
        --></columns>
      </block-inner>
    </block>

  </div>

  <div data-ko-container="main" data-ko-wrap="false">

    <block name="logoBlock" label="Logo Block">
      <block-inner if="externalBackgroundVisible">
        <content width="276" width-prop="18 + Math.round(imageWidth)">
          <linked-img width="258" placeholder-height="150" width-prop="imageWidth" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C150" editable="image"></linked-img>
        </content>
      </block-inner>
      <block-inner false="" if="externalBackgroundVisible eq false">
        <content width="276" width-prop="18 + Math.round(imageWidth)">
          <linked-img width="258" placeholder-height="150" width-prop="imageWidth" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C150" editable="image"></linked-img>
        </content>
      </block-inner>
    </block>

    <block name="sideArticleBlock" label="Image+Text Block">
      <block-inner>
        <columns dir="ltr" dir-prop="imagePos eq 'left' ? 'ltr' : 'rtl'"><!--
          --><column-content dir="ltr" width="184" width-prop="18 + Math.round(imageWidth)">
            <linked-img editable="image" width="166" width-prop="imageWidth" placeholder-height="130" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C130"></linked-img>
          </column-content><!--
          --><column-content dir="ltr" width="368" width-prop="552 - 18 - Math.round(imageWidth)">
            <text editable="titleText" if="titleVisible">Title</text>
            <long-text editable="longText"><p>Far far away, behind the word mountains, far from the countries <a href="">Vokalia and Consonantia</a>, there live the blind texts. Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean. A small river named Duden flows by their place and supplies it with the necessary regelialia.</p></long-text>
            <btn editable="buttonLink" if="buttonVisible" padding-top="2">BUTTON</btn>
          </column-content><!--
        --></columns>
      </block-inner>
    </block>

    <block name="singleArticleBlock" label="Image/Text Block">
      <block-inner>
        <content>
          <linked-img if="imageVisible" editable="image" width="534" placeholder-height="150" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=534%2C150" padding-bottom="8"></linked-img>
          <text editable="text" if="titleVisible">Section Title</text>
          <long-text editable="longText"><p>Far far away, behind the word mountains, far from the countries <a href="">Vokalia and Consonantia</a>, there live the blind texts. Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean. A small river named Duden flows by their place and supplies it with the necessary regelialia.</p></long-text>
          <btn editable="buttonLink" if="buttonVisible" padding-top="2">BUTTON</btn>
        </content>
      </block-inner>
    </block>

    <block name="titleBlock" label="Title Block">
      <block-inner class="gutterless fixedhalfgutter">
        <content>
          <title-text editable="text" align="center" size="22px" style="bigTitleStyle">Section Title</title-text>
        </content>
      </block-inner>
    </block>

    <block name="textBlock" label="Text Block">
      <block-inner>
        <content>
          <long-text editable="longText"><p>Far far away, behind the word mountains, far from the countries <a href="">Vokalia and Consonantia</a>, there live the blind texts.</p><p>Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean. A small river named Duden flows by their place and supplies it with the necessary regelialia.</p></long-text>
        </content>
      </block-inner>
    </block>

    <block name="tripleArticleBlock" label="3 Columns Block">
      <block-inner class="multicolumns">
        <columns horizontal-inner-padding="9"><repeater repeat="left,middle,right" height="90,70,110"><!--
          --><column-content width="184">
            <linked-img if="imageVisible and fixedImageHeightVisible" editable="{{repeat}}Image" width="166" height="90" height-prop="imageHeight" padding-bottom="8" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C90"></linked-img>
            <linked-img false="" if="imageVisible and fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="166" placeholder-height="{{height}}" padding-bottom="8" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C{{height}}"></linked-img>
            <text editable="{{repeat}}TitleText" if="titleVisible">Title</text>
            <long-text editable="{{repeat}}LongText"><p>Far far away, behind the word mountains, far from the countries <a href="">Vokalia and Consonantia</a>, there live the blind texts.</p></long-text>
            <btn editable="{{repeat}}ButtonLink" if="buttonVisible" padding-top="2">BUTTON</btn>
          </column-content><!--
        --></repeater></columns>
      </block-inner>
    </block>

    <block name="doubleArticleBlock" label="2 Columns Block">
      <block-inner class="multicolumns">
        <columns horizontal-inner-padding="9"><repeater repeat="left,right" height="90,120"><!--
            --><column-content width="276">
              <linked-img if="imageVisible and fixedImageHeightVisible" editable="{{repeat}}Image" width="258" height="100" height-prop="imageHeight" padding-bottom="8" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C100"></linked-img>
              <linked-img false="" if="imageVisible and fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="258" placeholder-height="{{height}}" padding-bottom="8" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C{{height}}"></linked-img>
              <text editable="{{repeat}}TitleText" if="titleVisible">Title</text>
              <long-text editable="{{repeat}}LongText"><p>Far far away, behind the word mountains, far from the countries <a href="">Vokalia and Consonantia</a>, there live the blind texts.</p></long-text>
              <btn editable="{{repeat}}ButtonLink" if="buttonVisible" padding-top="2">BUTTON</btn>
            </column-content><!--
          --></repeater></columns>
      </block-inner>
    </block>

    <block name="hrBlock" label="Separator Block">
      <block-inner class="gutterless fixedhalfgutter">
        <content>
          <divider width="100" height="1" style="hrStyle"></divider>
        </content>
      </block-inner>
    </block>

    <block name="buttonBlock" label="Button Block">
      <block-inner>
        <content>
          <btn editable="link" inner-padding="12" align="center" inner-hpadding="14" size="22px" style="bigButtonStyle">BUTTON</btn>
        </content>
      </block-inner>
    </block>

    <block name="imageBlock" label="Image">
      <block-inner if="gutterVisible eq false" class="gutterless">
        <linked-img editable="image" width="570" placeholder-height="200" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=570%2C200"></linked-img>
      </block-inner>
      <block-inner false="" if="gutterVisible">
        <content>
          <linked-img editable="image" width="534" placeholder-height="280" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=534%2C280"></linked-img>
        </content>
      </block-inner>
    </block>

    <block name="doubleImageBlock" label="Two Image Gallery">
      <block-inner if="gutterVisible eq false" class="gutterless">
        <columns width="570"><repeater repeat="left,right"><!--
            --><column-content width-breakpoint="570" width="285">
              <linked-img if="fixedImageHeightVisible" editable="{{repeat}}Image" width="285" height="180" height-prop="imageHeight" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=285%2C180"></linked-img>
              <linked-img false="" if="fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="285" placeholder-height="180" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=285%2C180"></linked-img>
            </column-content><!--
          --></repeater></columns>
      </block-inner>
      <block-inner false="" if="gutterVisible">
        <columns><repeater repeat="left,right"><!--
            --><column-content width="276">
              <linked-img if="fixedImageHeightVisible" editable="{{repeat}}Image" width="258" height="180" height-prop="imageHeight" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C180"></linked-img>
              <linked-img false="" if="fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="258" placeholder-height="180" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=258%2C180"></linked-img>
            </column-content><!--
      --></repeater></columns>
      </block-inner>
    </block>

    <block name="tripleImageBlock" label="Three Image Gallery">
      <block-inner if="gutterVisible eq false" class="gutterless">
        <columns width="570"><repeater repeat="left,middle,right"><!--
            --><column-content width-breakpoint="570" width="190">
              <linked-img if="fixedImageHeightVisible" editable="{{repeat}}Image" width="190" height="160" height-prop="imageHeight" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=190%2C160"></linked-img>
              <linked-img false="" if="fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="190" placeholder-height="160" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=190%2C160"></linked-img>
            </column-content><!--
       --></repeater></columns>
      </block-inner>
      <block-inner false="" if="gutterVisible">
        <columns><repeater repeat="left,middle,right"><!--
            --><column-content width="184">
              <linked-img if="fixedImageHeightVisible" editable="{{repeat}}Image" width="166" height="160" height-prop="imageHeight" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C160"></linked-img>
              <linked-img false="" if="fixedImageHeightVisible eq false" editable="{{repeat}}Image" width="166" placeholder-height="160" src="https://mosaico.io/srv/f-default/img?method=placeholder&amp;params=166%2C160"></linked-img>
            </column-content><!--
          --></repeater></columns>
      </block-inner>
    </block>

    <block name="bigSocialBlock" label="Big Social Block">
      <block-inner>
        <content>
          <custom-row align="center" class="links-color socialLinks mobile-textcenter">
            <repeater prefix="fb,tw,gg,web,wa,tg,in,pi,fl,vi,inst,you" default="if,if,if,false if,false if,false if,if,false if,false if,false if,if,if" color="#3b5998,#4099FF,#d34836,#606060,#25d366,#2da5e1,#007bb6,#C92228,#ff0084,#45bbff,#bc2a8d,#cd201f" name="Facebook,Twitter,Google,Web,Whatsapp,Telegram,Linkedin,Pinterest,Flickr,Vimeo,Instagram,Youtube">
              <social-icon {{default}}="{{prefix}}Visible" href-prop="{{prefix}}Url" size="48" size-prop="bigSocialIconSize" src="img/icons/{{prefix}}-rdcol-96.png" src-prop="Url.templatePath('img/icons/{{prefix}}-'+bigSocialIconType+'-96.png')" alt="{{name}}"></social-icon>
            </repeater>
          </custom-row>
        </content>
      </block-inner>
    </block>

    <block name="shareBlock" label="Share Block">
      <block-inner if="shareButtonType eq 'reverse'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
            <column-content {{default}}="{{prefix}}Visible" align="center">
              <share-btn href="{{shareurl}}%5Bpermlink_urlenc%5D" color="#FFFFFF" bgcolor="{{color}}" src="img/icons/{{prefix}}-white-96.png" btn-style="shareButtonStyle" editable="{{prefix}}Text">Share</share-btn>
            </column-content>
          </repeater>
        </columns>
      </block-inner>
      <block-inner false="" if="shareButtonType eq 'simple'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
            <column-content {{default}}="{{prefix}}Visible" align="center">
              <share-btn href="{{shareurl}}%5Bpermlink_urlenc%5D" color="#000000" bgcolor="#FFFFFF" src="img/icons/{{prefix}}-coloured-96.png" font-style="shareButtonStyle" editable="{{prefix}}Text">Share</share-btn>
            </column-content>
          </repeater>
        </columns>
      </block-inner>
      <block-inner false="" if="shareButtonType eq 'custom'">
        <columns autowidth="1">
          <repeater prefix="fb,tw,in,gg,pi" default="if,if,false if,if,false if" name="Facebook,Twitter,LinkedIn,Google+,Pinterest" color="#3b5998,#4099FF,#007bb6,#d34836,#C92228" shareurl="http://www.facebook.com/sharer/sharer.php?u=,http://twitter.com/share?url=,http://www.linkedin.com/shareArticle?url=,http://plus.google.com/share?url=,http://pinterest.com/pin/find/?url=">
            <column-content {{default}}="{{prefix}}Visible" align="center">
              <share-btn href="{{shareurl}}%5Bpermlink_urlenc%5D" src="img/icons/{{prefix}}-black-96.png" src-prop="Url.templatePath('img/icons/{{prefix}}-'+shareButtonStyle.iconColorType+'-96.png')" editable="{{prefix}}Text" custom-style="shareButtonStyle">Share</share-btn>
            </column-content>
          </repeater>
        </columns>
      </block-inner>
    </block>

    <block height="24" height-prop="spacerSize" td-styles="font-size:1px; height: 1px" name="spacerBlock" label="Spacer Block">
      <block-inner false="" if="externalBackgroundVisible eq false" class="gutterless">
        <spacer height="24" height-prop="spacerSize"></spacer>
      </block-inner>
      <div data-ko-height="spacerSize" style="height: 24px; -ko-height: @[spacerSize]px" data-ko-display="externalBackgroundVisible" data-ko-wrap="false">&nbsp;</div>
    </block>

    <block name="socialBlock" label="Social Block" theme="frameTheme">
      <block-inner inner-padding="9" class="multicolumns">
        <columns vertical-inner-padding="9"><!--
          --><column-content width="276">
            <long-text fixed-align="left" editable="longText"><p>Address and <a href="">Contacts</a></p></long-text>
          </column-content><!--
          --><column-content width="276">
            <custom-row if="socialIconType eq 'colors'" align="right" class="links-color socialLinks mobile-textcenter">
              <repeater prefix="fb,tw,gg,web,wa,tg,in,pi,fl,vi,inst,you" default="if,if,if,false if,false if,false if,false if,false if,false if,false if,false if,false if" color="#3b5998,#4099FF,#d34836,#606060,#25d366,#2da5e1,#007bb6,#C92228,#ff0084,#45bbff,#bc2a8d,#cd201f" name="Facebook,Twitter,Google,Web,Whatsapp,Telegram,Linkedin,Pinterest,Flickr,Vimeo,Instagram,Youtube">
                <social-icon {{default}}="{{prefix}}Visible" href-prop="{{prefix}}Url" color="{{color}}" src="img/icons/{{prefix}}-colors-96.png" alt="{{name}}"></social-icon>
              </repeater>
            </custom-row>
            <custom-row false="" if="socialIconType eq 'bw'" align="right" class="links-color socialLinks mobile-textcenter">
              <repeater prefix="fb,tw,gg,web,wa,tg,in,pi,fl,vi,inst,you" default="if,if,if,false if,false if,false if,false if,false if,false if,false if,false if,false if" color="#3b5998,#4099FF,#d34836,#606060,#25d366,#2da5e1,#007bb6,#C92228,#ff0084,#45bbff,#bc2a8d,#cd201f" name="Facebook,Twitter,Google,Web,Whatsapp,Telegram,Linkedin,Pinterest,Flickr,Vimeo,Instagram,Youtube">
                <social-icon {{default}}="{{prefix}}Visible" href-prop="{{prefix}}Url" color="#818181" src="img/icons/{{prefix}}-bw-96.png" alt="{{name}}"></social-icon>
              </repeater>
            </custom-row>
          </column-content><!--
        --></columns>
      </block-inner>
    </block>

  </div>

  <block name="footerBlock" label="Unsubscribe Block" theme="frameTheme">
    <block-inner>
      <content>
        <long-text editable="longText" fixed-align="center"><p>Email sent to <a href="mailto:[mail]">[mail]</a></p></long-text>
        <linked-text align="center" href="[unsubscribe_link]" editable="disiscrivitiText" color-prop="Color.readability(linkStyle.color, backgroundColor) gt 2 ? linkStyle.color : (Color.isReadable('#ffffff', backgroundColor) ? '#ffffff' : '#000000')">Unsubscribe</linked-text>
        <linked-img width="170" align="center" false="" if="_root_.sponsor.visible" content="_root_.sponsor" src="img/sponsor.gif" alt="sponsor"></linked-img>
      </content>
    </block-inner>
  </block>
</center><!--[if !(gte mso 15)]--></body><!--<![endif]--></html>